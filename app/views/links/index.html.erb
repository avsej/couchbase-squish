<div class="full-width squish-wrapper">
  <div class="container">
      <%= render :partial => "form" %>
  </div>
</div>

<%= session["session_id"] %>

<h3>Popular</h3>
<%- if @links.size > 0 -%>
<div class="container">
    <table>
      <thead>
        <th>Shortened Link</th>
        <th>Views</th>
        <th>Long Link</th>
      </thead>
      <tbody>
        <%= render @links %>
      </tbody>
    </table>
</div>
<%- end -%>

<h3>My Links</h3>
<%- if @my_links.size > 0 -%>
<div class="container">
    <table>
      <thead>
        <th>Shortened Link</th>
        <th>Views</th>
        <th>Long Link</th>
      </thead>
      <tbody>
        <%= render @my_links %>
      </tbody>
    </table>
</div>
<%- end -%>